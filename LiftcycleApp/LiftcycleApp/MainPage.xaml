<Page
    x:Class="LiftcycleApp.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:LiftcycleApp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <Button Margin="0,0,20,0" x:Name="HomeButton" Click="HomeButton_Click" Content="Home"/>
            <Button Margin="0,0,20,0" x:Name="BackButton" Click="BackButton_Click" Content="Back"/>
            <Button Margin="0,0,20,0" x:Name="ForwardButton" Click="ForwardButton_Click" Content="Forward"/>
            <Button Margin="0,0,20,0" x:Name="Navigation" Content="Navigate Root Frame" Click="Navigation_Click" />
        </StackPanel>
        <Frame x:Name="MyFrame">
        </Frame>
    </StackPanel>

</Page>
